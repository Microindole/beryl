// Sprint 15: Test Result builtin method calls - is_ok()

struct Error {
    int code
}

int! divide(int a, int b) {
    if b == 0 {
        return Err(Error { code: 1 })
    }
    return Ok(a / b)
}

int main() {
    var res1 = divide(10, 2)
    
    // 使用内置的 is_ok() 方法 (编译器实现)
    if res1.is_ok() {
        print("Test: is_ok works!\n")
    }
    
    return 0
}
